.listContainer {
  display: flexbox;
  flex-direction: column;
  background-color: #FFF;
  margin: 20rpx 20rpx;
}

.listContainer .listDesc {
  display: flex;
  justify-content: space-between;
  border-bottom: 2rpx solid;
}

.listContainer .listDetail {
  display: flex;
  justify-content: space-between;
  flex-direction: row;
}

.listContainer .listItem {
  display: flex;
  flex-direction: column;
}